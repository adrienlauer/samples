[my-app]
key = value
macro = *${my-app.key}*
environment-variable = ${env:JAVA_HOME}
system-property = ${sys:java.version}
country = FR

[org.seedstack.seed.security.roles]
admin = ADMIN.{location}
editor = EDITOR
guest = *

[org.seedstack.seed.security.permissions]
admin = user:*, article:*
editor = article:create, article:edit, article:delete

[org.seedstack.seed.security.users]
alice = password, ADMIN.FR
bob = password, ADMIN.DE
carol = password, EDITOR
dave = password
